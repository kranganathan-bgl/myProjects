			<xs:extension base="PSCStatementNotificationType">
				<xs:sequence>
					<xs:element name="WithdrawalDate" type="xs:date"/>
					<xs:element name="RestrictionsNoticeWithdrawalReason" type="RestrictionsNoticeWithdrawalReasonType" minOccurs="0">
