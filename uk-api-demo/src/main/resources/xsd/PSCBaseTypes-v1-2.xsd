								<xs:complexType>
									<xs:sequence>
										<xs:element ref="LegalPersonName" minOccurs="0"/>
										<xs:element name="Address" type="CompanyAddressType" minOccurs="0"/>
										<xs:element name="LegalPersonIdentification" type="LegalPersonIdentificationType" minOccurs="0"/>
										<xs:choice minOccurs="0">
											<xs:element name="NatureOfControls" type="NatureOfControlsType">
												<xs:annotation>
													<xs:documentation>NatureOfControls supplied will overwrite the previous nature of controls.</xs:documentation>
												</xs:annotation>
											</xs:element>
