						<xs:maxLength value="160"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LegalForm">
				<xs:annotation>
					<xs:documentation>The legal form of the company as defined its country of registration</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="nonEmptyOtherDataString">
						<xs:minLength value="1"/>
						<xs:maxLength value="160"/>
						<xs:whiteSpace value="collapse"/>
