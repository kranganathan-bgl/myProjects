	<xs:element name="SuperSecureIndividual">
		<xs:annotation>
			<xs:documentation>Individual PSC with 790ZG protection.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
