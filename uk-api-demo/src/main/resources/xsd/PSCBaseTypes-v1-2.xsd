			<xs:element ref="Forename" minOccurs="0"/>
			<xs:element ref="OtherForenames" minOccurs="0"/>
			<xs:element name="PartialDOB" type="PartialDOBType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
